	</ItemGroup>
	<ItemGroup>
		<PackageReference Include="Rebus" Version="[6.0.0,)" />
	</ItemGroup>
	<ItemGroup>
		<PackageReference Include="Microsoft.Extensions.Hosting.Abstractions" Version="[3.0.0,7)" />
	</ItemGroup>
